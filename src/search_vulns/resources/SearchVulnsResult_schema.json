ies": {"description": "Reference where the data source tracks this vulnerability", "minLength": 1, "type": "string"}, "description": "Stores which data sources keep track of this vulnerability and where", "minProperties": 1, "title": "Tracked By", "type": "object"}, "matched_by": {"additionalProperties": {"$ref": "#/$defs/Match"}, "description": "Stores by which data sources the vulnerability was matched by", "minProperties": 1, "title": "Matched By", "type": "object"}, "reported_patched_by": {"description": "Stores by which data sources the matched to product is considered patched against the vulnerability", "items": {"anyOf": [{"$ref": "#/$defs/DataSource"}, {"description": "Textual identifier for the data source", "minLength": 1, "type": "string"}]}, "minItems": 1, "title": "Reported Patched By", "type": "array", "uniqueItems": true}, "description": {"default": "", "description": "Description of the vulnerability", "title": "Description", "type": "string"}, "published": {"default": null, "description": "Publish date of the vulnerability", "format": "date-time", "title": "Published", "type": "string"}, "modified": {"default": null, "description": "Modified date of the vulnerability", "format": "date-time", "title": "Modified", "type": "string"}, "severity": {"additionalProperties": {"anyOf": [{"$ref": "#/$defs/SeverityCVSS"}, {"$ref": "#/$defs/SeverityEPSS"}]}, "description": "Stores different severity types for the vulnerability", "propertyNames": {"$ref": "#/$defs/SeverityType"}, "title": "Severity", "type": "object"}, "cwe_ids": {"description": "Stores CWE-IDs for the vulnerability", "items": {"type": "string"}, "title": "Cwe Ids", "type": "array", "uniqueItems": true}, "exploits": {"description": "Stores exploits references", "items": {"description": "Exploit URL", "minLength": 1, "type": "string"}, "title": "Exploits", "type": "array", "uniqueItems": true}, "cisa_kev": {"default": false, "description": "Stores if the vulnerability is a CISA KEV", "title": "Cisa Kev", "type": "boolean"}, "aliases": {"additionalProperties": {"description": "Reference or URL", "minLength": 1, "type": "string"}, "description": "Stores different IDs (aliases) for the vulnerability, alongside references with more details about the vulnerability and its ID", "propertyNames": {"description": "Alias / vulnerability ID", "minLength": 1}, "title": "Aliases", "type": "object"}, "misc": {"additionalProperties": true, "description": "Other optional information about the vulnerability", "title": "Misc", "type": "object"}}, "required": ["id", "match_reason", "tracked_by", "matched_by"], "title": "Vulnerability Model", "type": "object"}}, "properties": {"schema_version": {"const": "1.0.0", "default": "1.0.0", "description": "Schema version of this model", "title": "Schema Version", "type": "string"}, "product_ids": {"$ref": "#/$defs/ProductIDsResult", "description": "Stores successfully matched product IDs"}, "vulns": {"additionalProperties": {"$ref": "#/$defs/Vulnerability"}, "description": "Stores retrieved vulnerabilities", "propertyNames": {"description": "Vulnerability ID", "minLength": 1}, "title": "Vulns", "type": "object"}, "pot_product_ids": {"$ref": "#/$defs/PotProductIDsResult", "description": "Stores other relevant product IDs that did not match fully"}, "version_status": {"$ref": "#/$defs/VersionStatusResult", "description": "Provides status about queried software version if available"}}, "title": "search_vulns Result Model", "type": "object"}
