{"$defs": {"DataSource": {"enum": ["nvd", "nvd++", "ghsa", "osv.dev", "debian", "ubuntu", "redhat", "product_specific", "other"], "title": "DataSource", "type": "string"}, "Match": {"properties": {"match_reason": {"$ref": "#/$defs/MatchReason", "description": "Reason for the match"}, "confidence": {"description": "Confidence for the match", "exclusiveMinimum": 0, "maximum": 1, "title": "Confidence", "type": "number"}}, "required": ["match_reason", "confidence"], "title": "Vulnerability Match Model", "type": "object"}, "MatchReason": {"enum": ["product_match", "version_in_range", "general_product_ok", "description_match", "general_product_uncertain", "single_higher_version", "vuln_id", "n_a"], "title": "MatchReason", "type": "string"}, "PotProductIDsResult": {"properties": {"cpe": {"description": "A list containing tuples of a CPE and a match score, sorted by abs(match score)", "items": {"maxItems": 2, "minItems": 2, "prefixItems": [{"type": "string"}, {"maximum": 1, "minimum": -1, "type": "number"}], "type": "array"}, "title": "Cpe", "type": "array"}, "purl": {"description": "A list containing tuples of a PURL and a match score, sorted by abs(match score)", "items": {"maxItems": 2, "minItems": 2, "prefixItems": [{"type": "string"}, {"maximum": 1, "minimum": -1, "type": "number"}], "type": "array"}, "title": "Purl", "type": "array"}, "raw": {"description": "A list containing tuples of a raw string / product ID and a match score, sorted by abs(match score)", "items": {"maxItems": 2, "minItems": 2, "prefixItems": [{"type": "string"}, {"maximum": 1, "minimum": -1, "type": "number"}], "type": "array"}, "title": "Raw", "type": "array"}}, "title": "search_vulns Potential ProductID Result Model", "type": "object"}, "ProductIDsResult": {"properties": {"cpe": {"description": "A list of CPEs", "items": {"type": "string"}, "title": "Cpe", "type": "array"}, "purl": {"description": "A list of PURLs", "items": {"type": "string"}, "title": "Purl", "type": "array"}, "raw": {"descrip